<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<VBox prefHeight="351.0" prefWidth="550.0" spacing="5.0" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controller.RenewLicenseController">
   <children>
      <HBox alignment="CENTER">
         <children>
            <Label fx:id="title" text="license">
               <styleClass>
                  <String fx:value="fw-500" />
                  <String fx:value="color-primary-dark" />
                  <String fx:value="h6_5" />
               </styleClass>
            </Label>
         </children>
      </HBox>
      <Separator prefWidth="200.0" />
      <HBox fx:id="prevInspection" alignment="CENTER_LEFT" prefHeight="31.0" prefWidth="540.0" spacing="5.0" />
      <HBox alignment="CENTER" prefHeight="116.0" prefWidth="540.0">
         <children>
            <GridPane prefHeight="133.0" prefWidth="308.0">
               <children>
                  <Label text="Renewal Date" />
                  <Label text="Next Renewal" GridPane.rowIndex="1" />
                  <DatePicker fx:id="renewalDate" promptText="dd-mm-yyyy" GridPane.columnIndex="1" />
                  <DatePicker fx:id="nextRenewal" promptText="dd-mm-yyyy" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="207.0" minWidth="8.0" prefWidth="8.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="199.0" minWidth="0.0" prefWidth="199.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
            </GridPane>
         </children>
      </HBox>
      <Separator prefWidth="200.0" />
      <Pane prefHeight="70.0" prefWidth="540.0" VBox.vgrow="ALWAYS" />
      <HBox alignment="CENTER_LEFT" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onClose" styleClass="btn" text="Close" />
            <Pane layoutX="112.0" layoutY="16.0" HBox.hgrow="ALWAYS" />
            <Button layoutX="16.0" layoutY="16.0" mnemonicParsing="false" onAction="#onSave" styleClass="btn-success" text="Save" />
         </children>
         <styleClass>
            <String fx:value="padding-5" />
            <String fx:value="border-light-3" />
         </styleClass>
      </HBox>
   </children>
   <styleClass>
      <String fx:value="padding-5" />
      <String fx:value="scene" />
   </styleClass>
</VBox>
