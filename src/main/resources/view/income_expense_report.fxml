<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox fx:id="container" alignment="TOP_CENTER" prefHeight="510.0" prefWidth="939.0" spacing="5.0" styleClass="scene"
       stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controller.IncomeExpenseReportDetailsController">
   <children>
      <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="date" prefWidth="75.0" text="Date" />
            <TableColumn fx:id="category" prefWidth="75.0" text="Category" />
            <TableColumn fx:id="regNumber" prefWidth="75.0" text="Reg Number" />
            <TableColumn fx:id="driver" prefWidth="75.0" text="Driver" />
            <TableColumn fx:id="conductor" prefWidth="75.0" text="Conductor" />
            <TableColumn fx:id="details" prefWidth="75.0" text="Details" />
          <TableColumn fx:id="amount" prefWidth="75.0" text="Amount" />
            <TableColumn fx:id="options" prefWidth="75.0" text="Options" />

        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</VBox>
